# history will ingore commands that start with a space
alias pwd=' pwd'
alias clear=' clear'

alias ls=' ls --color=auto'
alias ll=' ls -alFh'
alias la=' ls -A'
alias l=' ls -F'

alias e=' eza'
alias el=' eza --long --all --header --group-directories-first --time-style=long-iso'

alias cz='chezmoi'
alias mbd='mamba deactivate'
alias mba='mamba activate'
